
public class Methods {
	public static void main(String[] args) {
		sayHelloWorld();
		
		sayHelloTo("Charles");
	}
	
	//create method
	static void sayHelloWorld() {
		System.out.println("Hello world!");
	}
	
	// param name
	static void sayHelloTo (String name) {
		System.out.println("Good morning, " + name +" !");
	}
}
